net=/usr/bin/net
samba_server=/usr/sbin/smbd
smbgroupedit=/usr/bin/smbgroupedit
swat=/usr/sbin/swat
dont_convert=-499
samba_status_program=/usr/bin/smbstatus
text_lists=0
